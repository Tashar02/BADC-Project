<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.mainproject.saad.U4VerifyContractorRegistrationController">
    <children>
        <Label alignment="CENTER" layoutX="182.0" layoutY="24.0" prefHeight="17.0" prefWidth="236.0" text="Contractor Registrations" />
      <TableView fx:id="applicationsTableView" layoutX="315.0" layoutY="101.0" prefHeight="260.0" prefWidth="271.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="94.0" text="Supplier ID" />
          <TableColumn fx:id="statusCol" prefWidth="88.0" text="Status" />
            <TableColumn fx:id="reportDateCol" prefWidth="88.0" text="Report Date" />
        </columns>
      </TableView>
      <Label layoutX="333.0" layoutY="66.0" prefHeight="17.0" prefWidth="70.0" text="Load Table:" />
      <Button layoutX="417.0" layoutY="62.0" mnemonicParsing="false" onAction="#loadAllOA" prefHeight="25.0" prefWidth="70.0" text="All" />
      <Button layoutX="498.0" layoutY="62.0" mnemonicParsing="false" onAction="#loadPendingOA" prefHeight="25.0" prefWidth="70.0" text="Pending" />
      <Button layoutX="527.0" layoutY="367.0" mnemonicParsing="false" onAction="#cancelOA" text="Cancel" />
      <Button layoutX="24.0" layoutY="62.0" mnemonicParsing="false" onAction="#detailsOA" prefHeight="25.0" prefWidth="252.0" text="Select From The Table and load details" />
      <Button layoutX="96.0" layoutY="336.0" mnemonicParsing="false" onAction="#approveOA" text="Approve" />
      <Button layoutX="225.0" layoutY="336.0" mnemonicParsing="false" onAction="#rejectOA" text="Reject" />
      <Label layoutX="24.0" layoutY="243.0" text="Remarks: " />
      <TextArea fx:id="remarksTF" layoutX="85.0" layoutY="243.0" prefHeight="83.0" prefWidth="200.0" />
      <Label fx:id="detailsLabel" layoutX="24.0" layoutY="101.0" prefHeight="129.0" prefWidth="252.0" />
    </children>
</AnchorPane>
